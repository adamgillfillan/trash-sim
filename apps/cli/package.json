{
  "name": "@trash-sim/cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "trash-sim": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "tsx src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --dir src/__tests__/cli-e2e"
  },
  "dependencies": {
    "@trash-sim/game-core": "0.1.0"
  }
}
